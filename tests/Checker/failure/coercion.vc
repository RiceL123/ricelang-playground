int main() {
    boolean b = true;
    int i = 1;
    float f = 1.0;

    // Boolean expressions
    i && i;
    f && f;
    f && i;
    i && f;
    i && b;
    b && i;
    f && b;
    b && f;
    i || i;
    f || f;
    f || i;
    i || f;
    i || b;
    b || i;
    f || b;
    b || f;

    // Relational expressions
    b + b;
    b + i;
    i + b;
    b + f;
    f + b;
    b - b;
    b - i;
    i - b;
    b - f;
    f - b;
    b * b;
    b * i;
    i * b;
    b * f;
    f * b;
    b / b;
    b / i;
    i / b;
    b / f;
    f / b;
    b < b;
    b < i;
    i < b;
    b < f;
    f < b;
    b <= b;
    b <= i;
    i <= b;
    b <= f;
    f <= b;
    b > b;
    b > i;
    i > b;
    b > f;
    f > b;
    b >= b;
    b >= i;
    i >= b;
    b >= f;
    f >= b;

    // Equality
    b == i;
    i == b;
    b == f;
    f == b;
    b != i;
    i != b;
    b != f;
    f != b;

    // Unary
    +b;
    +b;
    -b;
    -b;

    return 1;
}
