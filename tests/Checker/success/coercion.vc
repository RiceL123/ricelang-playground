void eB(boolean b) {}
void eF(float f) {}

int main() {
    boolean b = true;
    int i = 1;
    float f = 1.0;
    int eI[1];

    // Boolean expressions
    eB(b && b);
    eB(b || b);

    // Relational expressions
    eI[i + i];
    eF(f + f);
    eF(f + i);
    eF(i + f);
    eI[i - i];
    eF(f - f);
    eF(f - i);
    eF(i - f);
    eI[i * i];
    eF(f * f);
    eF(f * i);
    eF(i * f);
    eI[i / i];
    eF(f / f);
    eF(f / i);
    eF(i / f);
    eB(i < i);
    eB(f < f);
    eB(f < i);
    eB(i < f);
    eB(i <= i);
    eB(f <= f);
    eB(f <= i);
    eB(i <= f);
    eB(i > i);
    eB(f > f);
    eB(f > i);
    eB(i > f);
    eB(i >= i);
    eB(f >= f);
    eB(f >= i);
    eB(i >= f);

    // Equality
    eB(b == b);
    eB(i == i);
    eB(f == f);
    eB(i == f);
    eB(f == i);
    eB(b != b);
    eB(i != i);
    eB(f != f);
    eB(i != f);
    eB(f != i);

    // Unary
    eI[+i];
    eF(+f);
    eI[-i];
    eF(-f);

    return 1;
}
