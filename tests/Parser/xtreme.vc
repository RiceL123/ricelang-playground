int mod(int a, int b) {
    return a - (a / b * b);
}

void main() {
    int n;
    int y;

    putString("Enter size: ");
    n = getInt();
    if (!mod(n, 2)) {
        putStringLn("Size must be odd");
        return 1;
    }

    for (y = 0; y < n; y = y + 1) {
        int x;
        for (x = 0; x < n; x = x + 1) {
            int ax = x;
            int ay = y;
            if (x > n / 2) {
                ax = n - x - 1;
            }
            if (y > n / 2) {
                ay = n - y - 1;
            }

            if (!mod(ay - ax, 4)) {
                putString("*");
            } else {
                putString("-");
            }
        }
        putLn();
    }
}
